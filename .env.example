# Syncoor Configuration used for docker-compose.yml
# Copy this file to .env and customize as needed

# Authentication token for server API (leave empty to disable auth)
SYNCOOR_AUTH_TOKEN=your-secret-token-here

# Log level (trace, debug, info, warn, error, fatal, panic)
LOG_LEVEL=info

# Server configuration
SYNCOOR_SERVER_ADDR=:8080

# Network configurations for sync clients
SYNCOOR_NETWORK=hoodi

# Client configurations
SYNCOOR_EL_CLIENT=geth
SYNCOOR_CL_CLIENT=teku
